<INTENT_2_L0TC> = L0TC__ verb_pattern_01__ (comprueba | verifica) [qué [(eventos | citas | encuentros)] (hay | tengo) en] ((mi | la) agenda | (mi | el) calendario) el <DATE>;

<INTENT_2_L1TC> = <INTENT_2_L1TC_1> | <INTENT_2_L1TC_2>;
<INTENT_2_L1TC_1> = L1TC__ verb_pattern_01__ (muestra | muéstrame) [qué [(eventos | citas | encuentros)] (hay | tengo) en] ((mi | la) agenda | (mi | el) calendario) el <DATE>;
<INTENT_2_L1TC_2> = L1TC__ verb_pattern_02__ (lee | léeme) [qué [(eventos | citas | encuentros)] (hay | tengo) en] ((mi | la) agenda | (mi | el) calendario) el <DATE>;

<INTENT_2_L2TC> = <INTENT_2_L2TC_1> | <INTENT_2_L2TC_2> | <INTENT_2_L2TC_3> | <INTENT_2_L2TC_4> | <INTENT_2_L2TC_5>;
<INTENT_2_L2TC_1> = L2TC__ verb_pattern_01__ dime qué hay en (mi | la) (agenda | lista de (tareas | quehaceres)) el <DATE>;
<INTENT_2_L2TC_2> = L2TC__ verb_pattern_02__ tengo (algunos eventos | algunas citas | algunos encuentros | planes) el <DATE>?;
<INTENT_2_L2TC_3> = L2TC__ verb_pattern_03__ (estoy ocupado | tengo tiempo) el <DATE>?;
<INTENT_2_L2TC_4> = L2TC__ verb_pattern_04__ hay (algunos eventos | algunas citas | algunos encuentros | planes) el <DATE>?;
<INTENT_2_L2TC_5> = L2TC__ verb_pattern_05__ (busca | búscame) (eventos | citas | encuentros | planes) el <DATE>;

<INTENT_2_REPHRASE> = <INTENT_2_REPHRASE_1> | <INTENT_2_REPHRASE_2>;
<INTENT_2_REPHRASE_1> = REPHRASE__ verb_pattern_01__ se me ha olvidado algo [hoy];
<INTENT_2_REPHRASE_2> = REPHRASE__ verb_pattern_02__ [dame] información sobre eventos estos días;

