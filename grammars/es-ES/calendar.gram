#JSGF V1.0;
#VUI-VERSION: 0.2.0;
public <S> = Calendar__ (OpenCalendar__ <INTENT_1> | CheckCalendarOnDate__ <INTENT_2> | CheckCalendarEventName__ <INTENT_3> | AddEventWithName__ <INTENT_4> | AddEventOnDateWithName__ <INTENT_5> | NotifyOnEventStart__ <INTENT_6> | NotNotifyOnEventStart__ <INTENT_7> | NotifyOnEventInLocation__ <INTENT_8> | NotNotifyOnEventInLocation__ <INTENT_9> | NotifyWhenEventNameStart__ <INTENT_10>);

#PRECONDITIONS
<WHEN_1> = cuándo empieza (el mítin | el evento | la cita | el encuentro) en mi calendario;

<WHEN_2> = cuándo empieza (el mítin | el evento | la cita | el encuentro) en mi calendario en la ubicación <LOCATION>;

<WHEN_3> = ((la fecha | [la fecha y] la hora | el tiempo) (del mítin | del evento | de la cita | del encuentro) | cuándo empieza (el mítin | el evento | la cita | el encuentro)) <EVENT_NAME> en mi calendario;

#INTENTS
##1) OPEN CALENDAR
<INTENT_1> = <INTENT_1_L0TC> | <INTENT_1_L1TC> | <INTENT_1_L2TC> | <INTENT_1_REPHRASE>;
<INTENT_1_L0TC> = L0TC__ verb_pattern_01__ (abre | ábreme) [mi] [app | aplicación] calendario;

<INTENT_1_L1TC> = <INTENT_1_L1TC_1> | <INTENT_1_L1TC_2>;
<INTENT_1_L1TC_1> = L1TC__ verb_pattern_01__ (muestra | muéstrame) [mi] [app | aplicación] calendario;
<INTENT_1_L1TC_2> = L1TC__ verb_pattern_02__ (revisa | revísame) [mi] [app | aplicación] calendario;

<INTENT_1_L2TC> = L2TC__ verb_pattern_01__ (lanza |lánzame) [mi] [app | aplicación] calendario;

<INTENT_1_REPHRASE> = <INTENT_1_REPHRASE_1> | <INTENT_1_REPHRASE_2> | <INTENT_1_REPHRASE_3>;
<INTENT_1_REPHRASE_1> = REPHRASE__ verb_pattern_01__ qué hay en (mi | la) (agenda | lista de (tareas | quehaceres)) [hoy | mañana];
<INTENT_1_REPHRASE_2> = REPHRASE__ verb_pattern_02__ qué (debo | tengo que) hacer (mi | la) [hoy | mañana];
<INTENT_1_REPHRASE_3> = REPHRASE__ verb_pattern_03__ ayúdame organizar mi (día | agenda | lista de (tareas | quehaceres)) [hoy | mañana];

##2) CHECK CALENDAR IN DATE
